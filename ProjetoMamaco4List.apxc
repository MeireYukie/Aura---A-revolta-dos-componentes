public class ProjetoMamaco4List {
    
    @AuraEnabled
    
    public static List<Compras__c> getLista() {
        
        return [SELECT Nome_do_Produto__c, Quantidade__c, Preco_Unitario__c, Preco_Total__c, Data_da_Compra__c  
                FROM Compras__c 
                ORDER BY Nome_do_Produto__c];
    }
}
